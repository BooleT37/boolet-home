{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "sourceMap": true,
    "baseUrl": ".",
    "types": ["react"],
    "paths": {
      "src/*": [ "src/*" ],
      "serverModels/*": [ "../server/src/models/*" ]
    }
  },
  "include": [
    "src/**/*",
    "../server/src/models/*.ts"
  ]
}